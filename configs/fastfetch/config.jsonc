{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
 "logo": {
  "source": "$(find \"${XDG_CONFIG_HOME:-$HOME/.config}/fastfetch/pngs/\" -name \"*.png\" | sort -R | head -1)",
  "height": 28,
  "padding": { "top": 3, "right": 4, "bottom": 1, "left": 4 }
},


  "display": {
    "separator": " : "
  },
  "modules": [
    {
      "type": "custom",
      "format": "  \u001b[90m               "  // muted gray circles for subtle decor
    },
    {
      "type": "custom",
      "format": ""
    },
    {
      "type": "custom",
      "format": "     My conscience is clean — I have never used it.  ",
      "keyColor": "90"  // dark gray text
    },
    {
      "type": "custom",
      "format": ""
    },
    {
      "type": "os",
      "key": "   OS",
      "keyColor": "245"  // medium gray
    },
    {
      "type": "kernel",
      "key": "   Kernel",
      "keyColor": "250"  // lighter gray
    },
    {
      "type": "packages",
      "key": "  󰏗 Packages",
      "keyColor": "245"
    },
    {
      "type": "display",
      "key": "  󱍜 Display",
      "keyColor": "244"
    },
    {
      "type": "wm",
      "key": "   WM",
      "keyColor": "246"
    },
    {
      "type": "terminal",
      "key": "   Terminal",
      "keyColor": "241"
    },
    {
      "type": "media",
      "key": "  󰝚 Music",
      "keyColor": "245"
    },
    {
      "type": "command",
      "key": "  󱦟 OS Age",
      "keyColor": "244",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    {
      "type": "uptime",
      "key": "  ",
      "keyColor": "245"
    },
    {
      "type": "custom",
      "format": "  󰊤 GitHub : Saltyfunnel",
      "keyColor": "244"
    },
    {
      "type": "battery",
      "key": "   Battery",
      "keyColor": "244"
    },
    "break",
    {
      "type": "title",
      "key": "   User",
      "keyColor": "250"
    },
    {
      "type": "custom",
      "format": ""
    },
    {
      "type": "cpu",
      "format": "{1}",
      "key": "   CPU",
      "keyColor": "246"
    },
    {
      "type": "gpu",
      "format": "{2}",
      "key": "   GPU",
      "keyColor": "245"
    },
    {
      "type": "gpu",
      "format": "{3}",
      "key": "   GPU Driver",
      "keyColor": "244"
    },
    {
      "type": "memory",
      "key": "   Memory",
      "keyColor": "246"
    },
    {
      "type": "custom",
      "format": ""
    },
    "break",
    {
      "type": "custom",
      "format": "  \u001b[90m               "
    },
    "break"
  ]
}
